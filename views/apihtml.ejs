<!DOCTYPE html>
<head>
    <title>
        Group Project API
    </title>
    <link rel="stylesheet" href="/static/stylesheets/styles.css" type="text/css" />
    <script></script>
</head>
<body>
    <div class="container">
        <h2>
            <%= data.Title %> 
        </h2>
        <p>
            <b>Year Released:</b>
            <%= data.Year %> 
            <br>
            <b>Rated:</b>
            <%= data.Rated %> 
            <br>
            <b>Date Released:</b>
            <%= data.Released %> 
            <br>
            <b>Runtime:</b>
            <%= data.Runtime %> 
            <br>
            <b>Genre:</b>
            <%= data.Genre %> 
            <br>
            <b>Director:</b>
            <%= data.Director %> 
            <br>
            <b>Movie Plot:</b>
            <%= data.Plot %> 
            <br>
            <b>Language:</b>
            <%= data.Language %> 
            <br>
            <b>Country:</b>
            <%= data.Country %> 
            <br>
            <b>Awards:</b>
            <%= data.Awards %> 
            <br>
            <img align= "left" src="<%= data.Poster %>" alt="<%= data.alt %>" width="300" height="300"  /> 
        </p>
    </div>
</body>